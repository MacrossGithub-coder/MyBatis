<!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.macross.entity.personMapper">
    <select id="queryPersonById" resultType="org.macross.entity.Person" parameterType="int">
        select * from person where id = #{id}
    </select>
</mapper>
